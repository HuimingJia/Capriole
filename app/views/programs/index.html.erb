<p id="notice"><%= notice %></p>

<h1>Programs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
      <th colspan="3">Name</th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= program.name %></td>
        <td><%= link_to 'Show', program %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
